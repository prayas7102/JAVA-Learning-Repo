<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">
	<servlet>
		<servlet-name>demoproject</servlet-name>
		<servlet-class>demoproject.AddServlet</servlet-class>
		<init-param>
			<param-name>addServletNameKey</param-name>
			<param-value>from add servlet</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>demoproject</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>name</param-name>
		<param-value>Context common for all servlet</param-value>
	</context-param>
	<servlet>
		<servlet-name>demoprojec</servlet-name>
		<servlet-class>demoproject.SquareServlet</servlet-class>
		<init-param>
			<param-name>squareServletNameKey</param-name>
			<param-value>from square servlet</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>demoprojec</servlet-name>
		<url-pattern>/sq</url-pattern>
	</servlet-mapping>
</web-app>